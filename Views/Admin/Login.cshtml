@model Technexa.Models.admin

<br />
<br />
<br />
<div class="container">
    <div class="flexdivLogin">
        <div class="flexdivLogininside">
            <div class="row">
                <div class="col-md-4">
                    <form asp-controller="Admin" asp-action="Login">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group">
                            <label asp-for="Username" class="control-label"></label>
                            <input asp-for="Username" class="form-control" required/>
                            <span asp-validation-for="Username" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="keyenter" class="control-label"></label>
                            <input type="password" asp-for="keyenter" class="form-control" required/>
                            <span asp-validation-for="keyenter" class="text-danger"></span>
                        </div>
                        <br />
                        <div class="form-group">
                            <input type="submit" value="LogIn" class="btn btn-primary" />
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>